{
    "name": "olsgreen/autotrader-api",
    "description": "This package provides a means easily of interacting with the Auto Trader API.",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Oliver Green",
            "email": "green2go@gmail.com"
        }
    ],
    "require": {
        "guzzlehttp/guzzle": "^6.0 || ^7.2",
        "symfony/options-resolver": "^5.0",
        "ext-json": "*",
        "php": ">=7.3",
        "optimus/onion": "^1.0",
        "psr/log": "^1.1"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.0",
        "mockery/mockery": "^1.3"
    },
    "autoload": {
        "psr-4": {
          "Olsgreen\\AutoTrader\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
          "Tests\\": "tests/"
        }
    }
}
